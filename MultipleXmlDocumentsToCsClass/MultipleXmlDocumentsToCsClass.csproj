<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<TargetFramework>net6.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<LangVersion>latest</LangVersion>
		<RepositoryUrl>https://github.com/zxgamekingdom/XmlGenerateCsClass</RepositoryUrl>
		<PackageId>MultipleXmlDocumentsToCsClass</PackageId>
		<PackageDescription>解析多个XML文档,并生成C#类</PackageDescription>
		<GenerateAppxPackageOnBuild>true</GenerateAppxPackageOnBuild>
		<Version>2022.5.9.1</Version>
		<Author>zhou.taurus</Author>
	</PropertyGroup>
	<ItemGroup>
		<PackageReference Include="Newtonsoft.Json" Version="13.0.1"/>
	</ItemGroup>
</Project>
